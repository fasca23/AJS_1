{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
            "no-restricted-syntax": [
                "error",
                "LabeledStatement",
                "WithStatement"
            ],
            "no-unused-vars": "warn",
            "no-multiple-empty-lines": "warn"
    }
}

